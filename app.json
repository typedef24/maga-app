    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "background": "#0000ff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
